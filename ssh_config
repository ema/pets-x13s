ForwardAgent no

VisualHostKey yes
VerifyHostKeyDNS ask
StrictHostKeyChecking ask

UserKnownHostsFile ~/.ssh/known_hosts
HashKnownHosts no

# Get public key with:
#   ssh-keygen -D /usr/lib/aarch64-linux-gnu/libykcs11.so -e
#
# To avoid typing the PIN every time, add it to ssh-agent:
#   ssh-add -s /usr/lib/aarch64-linux-gnu/libykcs11.so
PKCS11Provider /usr/lib/aarch64-linux-gnu/libykcs11.so
